{
    "manifest_version": 2,
    "name": "CounterAI",
    "version": "1.0",
    "background": {
        "scripts": ["backend/counterAI.py"],
        "persistent": false
    },
    "permissions": [
        "activeTab",
        "http://localhost/*",
        "http://127.0.0.1:8080/",
        "http://127.0.0.1:8081/"
    ],
    "browser_action": {
        "default_popup": "frontend/build/index.html",
        "default_icon": "icon.png"
    },
    "icons":{
        "128":"icon.png"
    }
}
  